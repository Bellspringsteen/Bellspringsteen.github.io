---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Labs Bell Blog</title>
 <link href="http://Bellspringsteen.github.com/atom.xml" rel="self"/>
 <link href="http://Bellspringsteen.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://Bellspringsteen.github.com</id>
 <author>
   <name>Labs Bell Blog</name>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://Bellspringsteen.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://Bellspringsteen.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

 <url><loc>{{ site.url }}/blog/</loc></url>


  {% for tag in site.data.tags %}
        <url><loc>{{ site.url }}/blog/tag/{{ tag.slug }}/</loc></url>
    {% endfor %}

</feed>
